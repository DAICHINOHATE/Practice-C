#include <stdio.h>

int main()
{
    int a = 2;
    int b = a << 1;

    printf("%d\n", b);

    return 0;
}


int Max(int a, int b)
{
    int max = 0;
    max = a > b ? a : b;
    return max;
}

int main()
{
    int a = 0;
    int b = 0;
    int num = 0;
    scanf("%d%d", &a, &b);
    num = Max(a, b);
    printf("max=%d\n", num);
    return 0;
}

