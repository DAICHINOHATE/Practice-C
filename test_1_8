#include<stdio.h>

int main()
{
    int i = 0;
    scanf("%d", &i);
    if (i % 2 == 1)
        printf("是奇数\n");
    else
        printf("不是奇数\n");
    return 0;
}


int main()
{
    int i = 1;
    while (i < 101)
    {
        if (i % 2 == 1)
            printf("%d\n", i++);
        else
            i++;
    }

    return 0;
}

